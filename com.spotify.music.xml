      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementService"/>
      <component-filter name="com.spotify.music/com.spotify.music.ads.voice.VoiceAdService"/>
      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.spotify.music/com.spotify.music.libs.adbasedondemand.service.AdBasedOnDemandService"/>
      <component-filter name="com.spotify.music/com.spotify.music.samsungpersonalization.customization.TpoContextChangedService"/>
