<rules>
   <activity block="true" log="false">
      <component-filter name="com.spotify.music/com.facebook.FacebookActivity"/>
      <component-filter name="com.spotify.music/com.facebook.CustomTabMainActivity"/>
      <component-filter name="com.spotify.music/com.facebook.CustomTabActivity"/>
      <component-filter name="com.spotify.music/com.spotify.mobile.android.ads.inappbrowser.AdWebViewActivity"/>
      <component-filter name="com.spotify.music/com.spotify.mobile.android.ads.inappbrowser.InAppBrowserLauncherActivity"/>
