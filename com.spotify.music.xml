      <component-filter name="com.spotify.music/com.spotify.music.ads.display.DisplayAdActivity"/>
      <component-filter name="com.spotify.music/com.spotify.nativeads.homeformats.impl.help.HelpWebViewActivity"/>
      <component-filter name="com.spotify.music/com.spotify.music.features.ads.cmp.CMPActivity"/>
