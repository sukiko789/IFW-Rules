<rules>
   <activity block="true" log="false">
      <component-filter name="com.skype.raider/com.facebook.ads.AudienceNetworkActivity"/>
   </activity>
   <broadcast block="true" log="false">
      <component-filter name="com.skype.raider/com.adjust.sdk.AdjustReferrerReceiver"/>
      <component-filter name="com.skype.raider/com.google.android.gms.measurement.AppMeasurementReceiver"/>
   </broadcast>
   <service block="true" log="false">
      <component-filter name="com.skype.raider/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.skype.raider/com.google.android.gms.measurement.AppMeasurementService"/>
   </service>
</rules>