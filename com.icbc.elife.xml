<rules>
   <activity block="true" log="false">
      <component-filter name="com.icbc.elife/com.huawei.android.pushselfshow.richpush.RichPushActivity"/>
   </activity>
   <broadcast block="true" log="false">
      <component-filter name="com.icbc.elife/com.icbc.elife.receiver.HuaweiPushReceiver"/>
