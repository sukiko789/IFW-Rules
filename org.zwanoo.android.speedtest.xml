<rules>
   <activity block="true" log="true">
      <component-filter name="org.zwanoo.android.speedtest/com.google.android.gms.ads.AdActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="org.zwanoo.android.speedtest/com.google.android.gms.analytics.AnalyticsReceiver"/>
      <component-filter name="org.zwanoo.android.speedtest/com.google.android.gms.tagmanager.InstallReferrerReceiver"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="org.zwanoo.android.speedtest/com.google.android.gms.analytics.AnalyticsJobService"/>
      <component-filter name="org.zwanoo.android.speedtest/com.google.android.gms.analytics.AnalyticsService"/>
   </service>
</rules>