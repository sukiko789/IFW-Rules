<rules>
   <activity block="true" log="true">
      <component-filter name="com.intsig.camscanner/com.google.android.gms.ads.AdActivity"/>
      <component-filter name="com.intsig.camscanner/com.intsig.camscanner.AdSimulatorActivity"/>
      <component-filter name="com.intsig.camscanner/com.inmobi.rendering.InMobiAdActivity"/>
      <component-filter name="com.intsig.camscanner/com.facebook.FacebookActivity"/>
      <component-filter name="com.intsig.camscanner/com.facebook.CustomTabMainActivity"/>
      <component-filter name="com.intsig.camscanner/com.facebook.CustomTabActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.intsig.camscanner/com.google.android.gms.analytics.AnalyticsReceiver"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.analytics.CampaignTrackingReceiver"/>
      <component-filter name="com.intsig.camscanner/com.facebook.CurrentAccessTokenExpirationBroadcastReceiver"/>
      <component-filter name="com.intsig.camscanner/com.vungle.warren.NetworkProviderReceiver"/>
      <component-filter name="com.intsig.camscanner/com.appsflyer.SingleInstallBroadcastReceiver"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.intsig.camscanner/com.google.android.gms.analytics.AnalyticsJobService"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.analytics.AnalyticsService"/>
      <component-filter name="com.intsig.camscanner/com.inmobi.ads.ApkDownloader$ApkDownloadService"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.measurement.AppMeasurementService"/>
      <component-filter name="com.intsig.camscanner/com.google.android.gms.analytics.CampaignTrackingService"/>
   </service>
</rules>