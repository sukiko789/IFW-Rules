<rules>
   <activity block="true" log="true">
      <component-filter name="com.speedsoftware.rootexplorer/com.dropbox.core.android.AuthActivity"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.box.androidlib.activities.BoxAuthentication"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.common.api.GoogleApiActivity"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.box.boxandroidlibv2.activities.OAuthActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.firebase.iid.FirebaseInstanceIdInternalReceiver"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.firebase.iid.FirebaseInstanceIdReceiver"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.measurement.AppMeasurementService"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.firebase.iid.FirebaseInstanceIdService"/>
      <component-filter name="com.speedsoftware.rootexplorer/com.google.android.gms.auth.api.signin.RevocationBoundService"/>
   </service>
</rules>