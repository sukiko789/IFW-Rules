<rules>
   <activity block="true" log="true">
      <component-filter name="com.devhd.feedly/com.facebook.FacebookActivity"/>
      <component-filter name="com.devhd.feedly/com.facebook.CustomTabMainActivity"/>
      <component-filter name="com.devhd.feedly/com.facebook.CustomTabActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.devhd.feedly/com.google.android.gms.analytics.AnalyticsReceiver"/>
      <component-filter name="com.devhd.feedly/com.facebook.CurrentAccessTokenExpirationBroadcastReceiver"/>
      <component-filter name="com.devhd.feedly/com.devhd.feedly.InstallReceiver"/>
      <component-filter name="com.devhd.feedly/com.google.android.gms.tagmanager.InstallReferrerReceiver"/>
      <component-filter name="com.devhd.feedly/com.devhd.feedly.ShareBroadcastReceiver"/>
      <component-filter name="com.devhd.feedly/com.devhd.feedly.widget.FeedlyWidgetProvider"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.devhd.feedly/com.google.android.gms.analytics.AnalyticsJobService"/>
      <component-filter name="com.devhd.feedly/com.google.android.gms.analytics.AnalyticsService"/>
   </service>
</rules>