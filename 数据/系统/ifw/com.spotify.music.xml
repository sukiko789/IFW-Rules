<rules>
   <activity block="true" log="true">
      <component-filter name="com.spotify.music/com.google.android.gms.ads.AdActivity"/>
      <component-filter name="com.spotify.music/com.facebook.CustomTabActivity"/>
      <component-filter name="com.spotify.music/com.facebook.CustomTabMainActivity"/>
      <component-filter name="com.spotify.music/com.facebook.FacebookActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"/>
      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="com.spotify.music/com.facebook.CurrentAccessTokenExpirationBroadcastReceiver"/>
      <component-filter name="com.spotify.music/com.google.firebase.iid.FirebaseInstanceIdReceiver"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.spotify.music/com.google.android.gms.measurement.AppMeasurementService"/>
      <component-filter name="com.spotify.music/com.google.firebase.components.ComponentDiscoveryService"/>
      <component-filter name="com.spotify.music/com.google.firebase.iid.FirebaseInstanceIdService"/>
      <component-filter name="com.spotify.music/com.spotify.mobile.android.service.flow.facebook.confirmation.FacebookUserConfirmationIntentService"/>
      <component-filter name="com.spotify.music/com.spotify.music.feedback.service.FeedbackService"/>
   </service>
</rules>