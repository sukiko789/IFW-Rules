<rules>
   <activity block="true" log="true">
      <component-filter name="com.google.android.apps.photos/com.google.android.gms.ads.AdActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.google.android.apps.photos/com.google.android.gms.analytics.AnalyticsReceiver"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.google.android.apps.photos/com.google.android.gms.analytics.AnalyticsJobService"/>
      <component-filter name="com.google.android.apps.photos/com.google.android.gms.analytics.AnalyticsService"/>
   </service>
</rules>