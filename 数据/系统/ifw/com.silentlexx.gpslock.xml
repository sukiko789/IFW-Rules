<rules>
   <activity block="true" log="true">
      <component-filter name="com.silentlexx.gpslock/com.google.android.gms.ads.AdActivity"/>
   </activity>
   <broadcast block="true" log="true">
      <component-filter name="com.silentlexx.gpslock/com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"/>
      <component-filter name="com.silentlexx.gpslock/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="com.silentlexx.gpslock/com.silentlexx.gpslock.receiver.BootReceiver"/>
      <component-filter name="com.silentlexx.gpslock/com.silentlexx.gpslock.receiver.ChargerReceiver"/>
      <component-filter name="com.silentlexx.gpslock/com.silentlexx.gpslock.Widget2"/>
      <component-filter name="com.silentlexx.gpslock/com.silentlexx.gpslock.Widget"/>
   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.silentlexx.gpslock/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.silentlexx.gpslock/com.google.android.gms.measurement.AppMeasurementService"/>
      <component-filter name="com.silentlexx.gpslock/com.google.firebase.components.ComponentDiscoveryService"/>
   </service>
</rules>