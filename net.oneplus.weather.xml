<rules>
   <broadcast block="true" log="false">
      <component-filter name="net.oneplus.weather/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="net.oneplus.weather/com.google.android.gms.measurement.AppMeasurementInstallReferrerReceiver"/>
   </broadcast>
   <service block="true" log="false">
      <component-filter name="net.oneplus.weather/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="net.oneplus.weather/com.google.android.gms.measurement.AppMeasurementService"/>
   </service>
</rules>