<rules>
   <activity block="true" log="false">
      <component-filter name="com.paypal.android.p2pmobile/com.google.android.gms.ads.AdActivity"/>
   </activity>
   <broadcast block="true" log="false">
      <component-filter name="com.paypal.android.p2pmobile/com.adjust.sdk.AdjustReferrerReceiver"/>
      <component-filter name="com.paypal.android.p2pmobile/com.google.android.gms.measurement.AppMeasurementReceiver"/>
      <component-filter name="com.paypal.android.p2pmobile/com.paypal.android.p2pmobile.appwidget.WidgetLauncherProvider"/>
   </broadcast>
   <service block="true" log="false">
      <component-filter name="com.paypal.android.p2pmobile/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.paypal.android.p2pmobile/com.google.android.gms.measurement.AppMeasurementService"/>
   </service>
</rules>