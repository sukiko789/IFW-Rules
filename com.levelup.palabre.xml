   </broadcast>
   <service block="true" log="true">
      <component-filter name="com.levelup.palabre/com.google.android.gms.analytics.AnalyticsJobService"/>
      <component-filter name="com.levelup.palabre/com.google.android.gms.analytics.AnalyticsService"/>
      <component-filter name="com.levelup.palabre/com.google.android.gms.measurement.AppMeasurementJobService"/>
      <component-filter name="com.levelup.palabre/com.google.android.gms.measurement.AppMeasurementService"/>
