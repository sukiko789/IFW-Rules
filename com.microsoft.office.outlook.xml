<rules>
<activity block="true" log="false">
  <component-filter name="com.microsoft.office.outlook/com.appnexus.opensdk.AdActivity"/>
  <component-filter name="com.microsoft.office.outlook/com.facebook.ads.AudienceNetworkActivity"/>
</activity>
<service block="true" log="false">
  <component-filter name="com.microsoft.office.outlook/com.google.android.gms.measurement.AppMeasurementService"/>
  <component-filter name="com.microsoft.office.outlook/com.google.android.gms.analytics.AnalyticsJobService"/>
