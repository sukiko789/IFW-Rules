  <component-filter name="com.microsoft.office.outlook/com.google.android.gms.analytics.AnalyticsService"/>
</service>
<broadcast block="true" log="false">
  <component-filter name="com.microsoft.office.outlook/com.google.android.gms.measurement.AppMeasurementReceiver"/>
  <component-filter name="com.microsoft.office.outlook/com.google.android.gms.analytics.AnalyticsReceiver"/>
